
# for profiler
profiler:
  enable: false
  interval: 60000 # ms


# for network
network:
  dump_packet: true
  dump_traffic: true


# for database
database:
  debug: true


# for abuse
abuse:
  validator: true


# for logging
logging:
  - output: console
    level: debug

  - output: file
    level: debug
    filename: service.log


# redis
redis_monitor:
  host: localhost
  port: 6379
  db: 0



redis_presences:
  - host: localhost
    port: 6379
    db: 1

  - host: localhost
    port: 6379
    db: 2


