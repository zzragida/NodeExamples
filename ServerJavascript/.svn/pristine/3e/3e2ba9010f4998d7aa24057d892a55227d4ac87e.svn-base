message Badges
{
	required int32 gift_count = 1;				// 선물함에 들어있는 선물의 개수
	required int32 blueprint = 2;					// 보유한 도면의 개수
 	required int32 costume_event = 3; 		// 만들수 있는 의상 + 강화할 수 있는 의상 
 	required int32 hearts_to_receive = 4;	// 친구가 보낸 하트 수
 	required int32 ask_friendships = 5; 	// 친구 등록 요청한 수
	required int32 achivement = 6;				// 100% 완료된 업적 수
}


message GameEvent
{
	required int32 exp = 1;							// 경험치 n배
	required int32 honbul = 2;					// 혼불 n배
	required int32 material = 3;				// 재료보상 n배
	required int32 start_timestamp = 4;	// 이벤트 시작시간(Unix Timestamp)
	required int32 end_timestamp = 5;		// 이벤트 종료시간(Unix Timestamp)
}


message Dungeons
{
	message Story
	{
		required int32 epic_progress = 1;	// 스토리 진행율
  	optional int32 last_epic_stage_id = 2;  // 마지막 에픽던전 스테이지 아이디
  	optional int32 last_epic_stage_difficulty = 3;  // 마지막 에픽던전 스테이지 난이도
		optional GameEvent event = 4;
	}

	message Daily
	{
		required int32 dungeon_id = 1;
		optional GameEvent event = 2;
	}

	message Survival
	{
		required int32 dungeon_id = 1;
		optional GameEvent event = 2;
	}

	message Event
	{
		required int32 dungeon_id = 1;
		optional GameEvent event = 2;
	}

	message Multi
	{
		required bool enable = 1;		// 진행가능여부
		required bool reward = 2;		// 보상지급여부
		optional int32 next_start_timestamp = 3;	// 다음 이벤트 시작시간
		optional int32 next_end_timestamp = 4;		// 다음 이벤트 종료시간
	}

	optional Story story = 1;				// 스토리 던전
