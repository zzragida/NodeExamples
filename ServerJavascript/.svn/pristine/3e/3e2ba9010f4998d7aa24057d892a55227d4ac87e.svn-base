message Attributes
{
  required int64 hp = 1;   // 체력
  required int64 mp = 2;   // 신력
  required int64 atk = 3;  // 공격력
  required double atr = 4;  // 최종 공격률
  required int64 def = 5;  // 방어력
  required double dfr = 6;  // 최종 방어율
  required double ctr = 7;  // 크리티컬 확률
  required double ctd = 8;  // 크리티컬 데미지
  required int64 ccm = 9;  // 체인 콤보
  required int64 dex = 10;  // 솜씨
  required int64 hpr = 11;  // 체력 회복량
  required double hpt = 12;  // 최력 회복 시간
  required int64 mpr = 13;  // 신력 회복량
  required double mpt = 14;  // 신력 회복 시간
  required double hb = 15;   // 혼불 보너스
  required double ctm = 16;  // 기술 쿨타임
  required double mov = 17;  // 이동력
  required double exp = 18;  // 경험치 보너스
}



message Hero
{
  message Equip
  {
    required int32 type = 1; 	// 장비 종류
    required int32 level = 2; // 장비 랭크
    required int32 overcome = 3; // 한계돌파
  }

  message Skill
  {
    required int32 type = 1; 				// 스킬 타입
    required int32 level = 2; 			// 스킬 레벨
  }

  required int32 job = 1;           // 직업
  required int32 level = 2;         // 레벨
  required int64 curr_exp = 3;           // 경험치
  required int64 next_exp = 4;      // 다음 레벨까지의 경험치
  required int64 playing_time = 5;  // 총 플레이 시간(ms)

  optional Equip weapon = 6;        // 무기
  optional Equip helmet = 7;        // 투구
  optional Equip shirt = 8;         // 상의
  optional Equip pants = 9;         // 하의
  optional Equip gloves = 10;        // 장갑
  optional Equip boots = 11;        // 부츠
  optional Equip ring = 12;         // 반지
  optional Equip necklece = 13;     // 목걸이

  repeated Skill skills = 14;        // 습득한 스킬들

  optional int32 costume_id = 15;    // 선택한 코스튬이 있을때

  optional int32 button_a = 16; // 버튼 A 에 할당된 기술
  optional int32 button_b = 17; // 버튼 B 에 할당된 기술
  optional int32 button_c = 18; // 버튼 C 에 할당된 기술
  optional bool expand_skill_button = 19; // 버튼 확장 여부

  required int32 epic_progress = 20; // 에픽 진행률
  required int32 skill_rate = 21; // 스킬 습득률
  required int64 skill_point = 22; // 스킬 포인트

  required Attributes attributes = 23; // 파라미터

	required int32 unlock_stage_count = 24;	// 하루중 스테이지를 잠금해제 할수 있는 횟수
}



message Inventory
{
  message Equip
  {
    required int32 level = 1;     // 장비 레벨
    required bool limited = 2;    // 장착 제한에 걸려있는지 여부
    optional bool broken = 3;     // 파괴된 아이템인지 여부
    required int32 overcome = 4;  // 한계돌파 수준
  }

  message Item
  {
    required int32 count = 1;     // 개수
  }

  message Entry
  {
    required int64 item_id = 1;
    required int32 type = 2;      // 아이템 종류
    optional Equip equip = 3;
    optional Item item = 4;
  }

  required int32 limit = 1;     // 인벤토리 제한 개수
  required int64 honbul = 2;		// 혼불
  required int64 cash = 3;			// 캐쉬
  repeated Entry entries = 4;		// 실제 들어있는 아이템(인벤토리 제한 개수보다 더 들어있을 수 있음)
  /* optional int32 fever = 5;  // 가차 피버 정보, 피버와 같이 요청시에만 처리 */
}



message GameInfo
{
  required int64 honbul = 1;
  required int64 cash = 2;
  required int64 talisman = 3; 				// 부적
  required int64 stone = 4; 					// 강화석
  required int64 coin = 5; 						// 강화석
	required int64 heart = 6;
  required int32 inventory_size = 7; 	// 인벤토리 슬롯 크기
  optional string nickname = 8;
  optional Hero selected = 9;
  required bool searchable = 11; 			// 이름 검색 가능 여부

  optional int32 ranking = 12; 				// 게임에서의 랭킹
  optional bool terminate_abnormally = 14; // 비정상 종료

	required int32 vip_level = 15;			// VIP 레벨

	optional Badges badges = 18;				// 뱃지정보
	optional Dungeons dungeons = 19;		// 던전정보

  required bool no_facebook_message = 22; // 페이스북 메시지 수신 여부
  required bool no_facebook_profile = 23; // 페이스북 프로필 표시 여부
	required bool review = 24;					 		// 댓글리뷰 보상여부
	required bool mycard = 25;							// MyCARD 상점이용 가능여부
}


