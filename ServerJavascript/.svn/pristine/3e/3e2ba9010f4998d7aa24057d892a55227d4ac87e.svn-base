message Stages
{
  enum Perfect 
  {
    PERFECT_NONE = 0;
    PERFECT_NO_DEATH = 1;
    PERFECT_NO_DAMAGE = 2;
  }

  message Stage
  {
    required int32 stage_id = 1; 		// 스테이지 아이디
    required bool lock = 2; 				// 레벨이 잠겨 있는지 여부
    optional bool clear = 3; 				// 스테이지 클리어 여부

    // 다음 도전 시간(초) 
    // (0 쿨타임이 끝남)
    optional int32 cooltime = 4; 
    optional int32 remain_cooltime_reset = 5;	// 남아있는 쿨타임 리셋 카운트
    
    message Level
    {
      required bool clear = 1; 		// 스테이지 클리어했는지 여부
      optional int32 star = 2; 		// 클리어 했을때 별
      optional int64 score = 3; 	// 클리어 했을 경우 점수
      required Perfect perfect = 4 [default = PERFECT_NONE]; // 노데스 클리어 여부
    }

    optional Level easy = 6; 			// 쉬움 난이도
    optional Level normal = 7; 		// 보통 난이도(오픈되었을때만 정보 있음)
    optional Level hard = 8; 			// 어려움 난이도(오픈되었을 때만 정보 있음)
    required int32 heart = 9;
  }

  required int32 dungeon_id = 1;		// 던전아이디
  repeated Stage stages = 2;				// 스테이지 정보
	optional GameEvent event = 3;			// 게임보상 이벤트
};


