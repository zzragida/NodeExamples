message Intercom
{
  enum MessageType
  {
    INVITE = 1;
    CLEAR_CACHE = 2;
		UPDATE_MONEY = 3;
		INSERT_GIFT = 4;
		DELETE_GIFT = 5;
		INSERT_ITEM = 6;
		DELETE_ITEM = 7;
	
	KICK = 100;
  }

  message ClearCache
  {
    required CacheType type = 1;
    required int32 action = 2;
  }

	message UpdateMoney
	{
		required int64 user_id = 1;
		optional int32 cash = 2;
		optional int32 heart = 3;
		optional int32 honbul = 4;
		optional int32 stone = 5;
		optional int32 talisman = 6;
		optional int32 coin = 7;
	}

	message InsertGift
	{
		required int64 user_id = 1;
		required int32 p_id = 2;				// 선물번호
	}

	message DeleteGift
	{
		required int64 user_id = 1;
		required int32 present_id = 2;	// 선물함의 선물아이디
	}

	message InsertItem
	{
		required int64 user_id = 1;
		required int32 item_no = 2;		// 아이템번호
	}

	message DeleteItem
	{
		required int64 user_id = 1;
		required int32 item_id = 2;		// 인벤의 아이템아이디
	}

	message Kick
	{
		required int64 user_id = 1;		// 사용자아이디
	}

	message Result
	{
		required bool ok = 1;
	}

  message Request
  {
    required MessageType type = 1;

    optional Invite invite = 2;
    optional ClearCache clear_cache = 3;
		optional UpdateMoney update_money = 4;
		optional InsertGift insert_gift = 5;
		optional DeleteGift delete_gift = 6;
		optional InsertItem insert_item = 7;
		optional DeleteItem delete_item = 8;

	optional Kick kick = 100;
  }

  message Response
  {
    required MessageType type = 1;

    optional InviteResult invite = 2;
		//optional Result clear_cache = 3;
		optional Result update_money = 4;
		optional Result insert_gift = 5;
		optional Result delete_gift = 6;
		optional Result insert_item = 7;
		optional Result delete_item = 8;

	optional Result kick = 100;
  }
}

